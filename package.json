{
  "name": "cleffa",
  "version": "0.3.0",
  "description": "CLI utility that parses argv and passes it into the default export of a file",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist/* && tsc && chmod +x ./dist/index.js && rm -rf dist/test",
    "link-self": "cd node_modules/.bin && ln -sf ../../dist/index.js cleffa",
    "test": "npm run link-self && vitest"
  },
  "keywords": [
    "cli",
    "argv",
    "parse",
    "flag",
    "run",
    "main",
    "exit",
    "exitCode",
    "status",
    "async",
    "Promise",
    "throw",
    "Error",
    "reject"
  ],
  "author": "Lily Skye <me@suchipi.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/resolve": "^1.20.6",
    "first-base": "^1.3.0",
    "nice-path": "^2.0.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suchipi/cleffa.git"
  },
  "dependencies": {
    "clef-parse": "^0.5.0",
    "kame": "^0.12.0",
    "pretty-print-error": "^1.1.1",
    "resolve": "^1.22.8"
  }
}
